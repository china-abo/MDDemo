<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.abo.mddemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".example.RecycleViewExampleActivity"/>
        <activity android:name=".example.EditTextExampleActivity"/>
        <activity android:name=".example.CardViewAvtivity"/>
        <activity android:name=".example.AppBarActivity"/>
        <activity android:name=".example.BottomTabActivity"/>
    </application>

</manifest>